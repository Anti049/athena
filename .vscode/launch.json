    "version": "0.2.0",
    "configurations": [
		{
			"name": "athena",
			"request": "launch",
			"type": "dart"
		},
		// Launch development flavor
		{
			"name": "athena (development)",
			"request": "launch",
			"type": "dart",
			"args": ["--flavor", "development"]
		},
		// Launch production flavor
		{
			"name": "athena (production)",
			"request": "launch",
			"type": "dart",
			"args": ["--flavor", "production"]
		},
		{
			"name": "athena (profile mode)",
			"request": "launch",
			"type": "dart",
			"flutterMode": "profile",
			"args": ["--flavor", "development"]
		},
		{
			"name": "athena (release mode)",
			"request": "launch",
			"type": "dart",
			"flutterMode": "release",
			"args": ["--flavor", "production"]
		}
	]
}