{
	// Use IntelliSense to learn about possible attributes.
	// Hover to view descriptions of existing attributes.
	// For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
	"version": "0.2.0",
	"configurations": [
		{
			"name": "athena",
			"request": "launch",
			"type": "dart"
		},
		// Launch development flavor
		{
			"name": "athena (development)",
			"request": "launch",
			"type": "dart",
			"args": [
				"--flavor",
				"development",
				"-t",
				"lib/main_development.dart"
			]
		},
		// Launch production flavor
		{
			"name": "athena (production)",
			"request": "launch",
			"type": "dart",
			"args": ["--flavor", "production", "-t", "lib/main_production.dart"]
		},
		{
			"name": "athena (profile mode)",
			"request": "launch",
			"type": "dart",
			"flutterMode": "profile",
			"args": [
				"--flavor",
				"development",
				"-t",
				"lib/main_development.dart"
			]
		},
		{
			"name": "athena (release mode)",
			"request": "launch",
			"type": "dart",
			"flutterMode": "release",
			"args": ["--flavor", "production", "-t", "lib/main_production.dart"]
		}
	]
}
